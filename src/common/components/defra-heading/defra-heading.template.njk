<div class="govuk-width-container">
  <main
    class="govuk-main-wrapper govuk-!-padding-top-4 govuk-!-padding-bottom-4 defra-banner"
  >
    <div class="govuk-grid-row">
      <div class="govuk-grid-column-one-third govuk-!-padding-left-0">
        <div class="defra-logo" data-testid="defra-heading-logo">
          <a href="/cases" class="defra-logo__link">
            <span class="govuk-visually-hidden"
              >Department for Environment Food &amp; Rural Affairs</span
            >
          </a>
        </div>
      </div>
      <div class="govuk-grid-column-two-thirds">
        <div
          class="defra-internal-service-name"
          data-testid="defra-heading-service-name"
        >
          <a
            href="/"
            title="Go to the homepage"
            class="defra-internal-service-name__link"
          >
            {{ params.serviceName }}
          </a>
        </div>
      </div>
    </div>
  </main>
</div>
